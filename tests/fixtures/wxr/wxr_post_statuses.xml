<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0"
    xmlns:wp="http://wordpress.org/export/1.2/"
    xmlns:content="http://purl.org/rss/1.0/modules/content/"
>
<channel>
    <item>
        <title>Publish Post</title>
        <wp:post_type>post</wp:post_type>
        <wp:status>publish</wp:status>
        <content:encoded><![CDATA[This is a published post.]]></content:encoded>
    </item>
    <item>
        <title>Draft Post</title>
        <wp:post_type>post</wp:post_type>
        <wp:status>draft</wp:status>
        <content:encoded><![CDATA[This is a draft post.]]></content:encoded>
    </item>
    <item>
        <title>Private Post</title>
        <wp:post_type>post</wp:post_type>
        <wp:status>private</wp:status>
        <content:encoded><![CDATA[This is a private post.]]></content:encoded>
    </item>
    <item>
        <title>Pending Post</title>
        <wp:post_type>post</wp:post_type>
        <wp:status>pending</wp:status>
        <content:encoded><![CDATA[This is a pending post.]]></content:encoded>
    </item>
    <item>
        <title>Future Post</title>
        <wp:post_type>post</wp:post_type>
        <wp:status>future</wp:status>
        <content:encoded><![CDATA[This is a future post.]]></content:encoded>
    </item>
</channel>
</rss>
